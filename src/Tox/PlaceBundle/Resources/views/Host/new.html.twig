{% extends 'ToxPlaceBundle:Host:host.html.twig' %}

{% block host %}
<h1>Host creation</h1>

<form action="{{ path('host_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('host') }}">
            Back to the list
        </a>
    </li>
</ul>
        {% endblock %}