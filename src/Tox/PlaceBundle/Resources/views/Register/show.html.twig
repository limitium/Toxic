{% extends 'ToxPlaceBundle:Register:register.html.twig' %}

{% block register %}
<h1>Register</h1>
<ul class="record_actions">
    <li>
        <a href="{{ path('register') }}">‚Üê</a>
    </li>
    <li>
        <a href="{{ path('register_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
</ul>
<h4><a class="ext_url" href="http://{{ entity.url }}">{{ entity.url }}</a></h4>
<div class="accounts">
    Accounts:
    <ul>
        {% for account in entity.Accounts %}
        <li>{{ account.username}} - {{ account.password}}</li>
        {% endfor %}
    </ul>
</div>
        {% endblock %}